{
    "name": "arminek/splendor-dojo",
    "description": "Splendor Coding Dojo",
    "type": "project",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "friends-of-behat/variadic-extension": "^1.3",
        "webmozart/assert": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Arkadiusz Krakowiak",
            "email": "arkadiusz.k.e@gmail.com"
        }
    ]
}
